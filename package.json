  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-preset-prometheusresearch": "^0.1.0",
    "doctoc": "^1.3.0",
    "eslint": "^3.19.0",
    "eslint-config-prometheusresearch": "^0.4.0",
    "eslint-plugin-flowtype": "^2.30.4",
    "eslint-plugin-react": "^6.10.3",
    "flow-bin": "^0.43.1",
    "mocha": "^3.2.0",
